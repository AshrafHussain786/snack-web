    "graphql": "^14.0.2",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.0",
    "hoist-non-react-statics": "^3.0.1",
    "js-cookie": "^2.2.0",
    "jsesc": "^2.5.1",
    "json-to-ast": "^2.0.5",
