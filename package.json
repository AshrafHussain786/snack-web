{
  "name": "@expo/web-snack",
  "version": "0.0.0",
  "private": true,
  "description": "The Snack website",
  "main": "build/server",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:dev": "nodemon --ignore src/client src/server/index.js --exec babel-node",
    "start:prod": "nodemon build/server/index.js",
    "domain": "hotel start && hotel add http://localhost:3011 --name snack.expo",
    "build": "cross-env NODE_ENV=production yarn build:server && yarn build:client",
    "build:server": "babel --source-maps --out-dir build/ src/",
    "build:client": "webpack",
    "clean": "yarn clean:server && yarn clean:client",
    "clean:server": "del build/",
    "clean:client": "del dist/",
    "test": "cross-env NODE_ENV=test jest --config jest/unit.config.js",
    "test:integration": "cross-env NODE_ENV=test jest --config jest/integration.config.js",
    "prestart:dev": "yarn domain",
    "prestart:prod": "yarn build",
    "prebuild": "yarn clean",
    "start-all": "./bin/start_server.sh"
  },
  "author": "Expo",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "universe/node",
      "universe/web"
    ]
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "aphrodite": "^2.2.3",
